@inject CarService Data

@* UI Section *@

<h3>Product List</h3>

@foreach(Car item in Products)
{
    <CarView product="item"></CarView>
}

@* Code Section *@
@code 
{

    [EditorRequired]
    [Parameter]
    public List<Car> Products { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        Products = CarService.GetProducts();
    }
}